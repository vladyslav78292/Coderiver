{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}